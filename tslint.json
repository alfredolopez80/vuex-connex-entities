{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "interface-name": false,
        "max-line-length":false,
        "no-namespace":false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules",
            "dist"
        ]
    }
}